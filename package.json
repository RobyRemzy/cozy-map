{
  "name": "cozy-map",
  "version": "0.0.1",
  "engines": [
    "node >= 0.10.0"
  ],
  "description": "Map with Leaflet JavaScript library.",
  "cozy-displayName": "Map",
  "cozy-type": "static",
  "cozy-permissions": {
    "App-Version": {
      "description": "0.0.1 A New Beginning"
    },
    "leaflet-locate": {
      "description": "ask permission to retrieve your location"
    }
  },
  "icon-path": "main_icon.png",
  "main": "src/app.js",
  "dependencies": {
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "uglify-js": "^2.4.17",
    "watchify": "^2.5.0",
    "corslite": "0.0.7",
    "leaflet": "0.7.7",
    "polyline": "0.2.0"
  },
  "scripts": {
    "start": "node_modules/.bin/watchify src/app.js -o ./build/browser-bundle.js -v -d .",
    "build": "./node_modules/.bin/browserify . | uglifyjs -cm > ./build/browser-bundle.js",
    "build-min": "NODE_ENV=production browserify . | uglifyjs -cm > ./build/browser-bundle.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RobyRemzy/cozy-map.git"
  },
  "keywords": [
    "cozy",
    "leaflet",
    "map"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robyremzy/cozy-map/issues"
  },
  "homepage": "https://github.com/robyremzy/cozy-map"
}
