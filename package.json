{
  "name": "cozy-map",
  "version": "0.1.32",
  "engines": [
    "*"
  ],
  "description": "Leaflet map suited for Cozy Platform.",
  "cozy-displayName": "Map",
  "cozy-color": "#C271DF",
  "cozy-type": "static",
  "cozy-permissions": {
    "User": {
      "description": "Map location with your Cozy Timezone."
    },
    "Contact": {
      "description": "Show contacts on map."
    }
  },
  "icon-path": "map.svg",
  "main": "src/app.js",
  "dependencies": {
  },
  "devDependencies": {
    "browserify": "9.0.3",
    "corslite": "0.0.7",
    "eslint": "2.8.0",
    "eslint-config-mourner": "2.0.1",
    "leaflet": "0.7.7",
    "leaflet.icon.glyph": "^0.2.0",
    "osm-auth": "^0.2.8",
    "osmtogeojson": "^2.2.11",
    "polyline": "0.2.0",
    "uglify-js": "2.4.17",
    "watchify": "2.5.0"
  },
  "scripts": {
    "start": "node_modules/.bin/watchify src/app.js -o ./build/map.min.js -v -d .",
    "build": "./node_modules/.bin/browserify . | uglifyjs -cm > ./build/map-bundle.js",
    "build-min": "NODE_ENV=production browserify . | uglifyjs -cm > ./build/map.min.js",
    "lint": "eslint src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RobyRemzy/cozy-map.git"
  },
  "keywords": [
    "cozy",
    "leaflet",
    "geocoder",
		"routing",
		"overpass",
    "map",
    "gis"
  ],
  "author": {
    "name": "Remi Roubin",
    "url": "https://github.com/RobyRemzy/"
  },
  "homepage": "https://github.com/robyremzy/cozy-map",
  "bugs": {
    "url": "https://github.com/robyremzy/cozy-map/issues"
  },
  "licenses": {
    "type": "Apache License 2.0",
    "url": "https://github.com/RobyRemzy/cozy-map/blob/master/LICENSE"
  }
}
